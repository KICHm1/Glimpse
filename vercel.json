{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {"src": "/(.*)",
      "dest": "/components/home/album/index.tsx"
    },
    {"src": "/album",
      "dest": "/components/home/album/index.tsx"
    },
    {"src": "/result",
      "dest": "/components/home/result/index.tsx"
    },
    {"src": "/(.*)",
      "dest": "/components/home/face/index.tsx"
    },
    {"src": "/(.*)",
      "dest": "/components/home/setting/index.tsx"
    }
  ]
}
