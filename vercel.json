{
  "version": 2,
  "builds": [
    {
      "src": "/index.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {"src": "/(.*)",
      "dest": "/"
    },
    {"src": "/album",
      "dest": "/"
    },
    {"src": "/result",
      "dest": "/"
    },
    {"src": "/setting",
      "dest": "/"
    },
    {"src": "/face  ",
      "dest": "/"
    }
  ]
}
